{
  "name": "smlr",
  "version": "0.0.3",
  "description": "Frontend framework",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dusanjovanov/smlr"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "size": "npx pkg-size",
    "types": "tsc",
    "test": "vitest run",
    "test-dev": "vitest"
  },
  "keywords": [
    "frontend",
    "ui",
    "router"
  ],
  "author": "Dusan Jovanov",
  "license": "MIT",
  "dependencies": {
    "@preact/signals-core": "^1.6.0",
    "clsx": "^2.1.0",
    "csstype": "^3.1.2",
    "history": "^5.3.0"
  },
  "devDependencies": {
    "prettier": "2.8",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2"
  }
}
